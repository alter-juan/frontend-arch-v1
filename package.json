{
  "name": "frontend-vue-arch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{ts,vue}": "eslint --max-warnings 1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test": "vitest --environment jsdom --run --root src/",
    "lint": "eslint src --ext .ts,.vue --fix",
    "prepare": "husky",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "firebase": "^10.11.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@pinia/testing": "^0.1.3",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.3",
    "@types/node": "^20.12.5",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vitest": "^1.4.0",
    "vue-tsc": "^1.8.27"
  }
}
